{
  "compilerOptions": {
    "resolveJsonModule": true,
    "incremental": true,
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "declarationMap": true,
    "outDir": "./docs/dist",
    "rootDir": "./",
    "composite": true,
    "removeComments": true,
    "downlevelIteration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "preserveSymlinks": true,
  },
  "files": [
    "./config.json",
    "./defs.ts",
    "./index.ts"
  ]
}
