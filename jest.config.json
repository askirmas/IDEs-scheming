{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/readJson.ts",
    "/index.ts"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "dist"
  ]
}