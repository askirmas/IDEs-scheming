{
  "json.schemas": [
    {
      "fileMatch": [
        "*.schema.*json"
      ],
      "url": "http://json-schema.org/draft-07/schema#"
    },
    {
      "fileMatch": [
        "jest.config.json"
      ],
      "url": "https://askirmas.github.io/jest.schema.json"
    },
    {
      "fileMatch": [
        "/scheming.config.json"
      ],
      "url": "./config.schema.json"
    },
    {
      "fileMatch": [
        "/parameters.json"
      ],
      "url": "./parameters.schema.json"
    }
  ]
}